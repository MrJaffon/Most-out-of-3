	int c;
	printf ("c = ");
	scanf ("%d",&c);
	int m;
	m = a;
	if (b>m)
	{
		b=m;
		if (c>m)
		{
			printf ("c-naibolshee ");
		}
		else
		{
			printf ("b-naibolshee ");
		}
	}
	else
	{
printf ("a-naibolshee ");
	}
